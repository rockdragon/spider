{
  "title": {
    "expr": "<h1[^>]*>([^<]+)",
    "g": 1
  },
  "publishDate": {
    "expr": "<li[^c]+class=\"time\">\\s+((\\d{4}-\\d{2}-\\d{2})|<script>([^<]+))",
    "g": 1
  },
  "overview": {
    "expr": "<div class=\"su_tit\">概况</div>\\s+<div class=\"su_con\">([^<]+)",
    "g": 1,
    "replace": {
      "a": "[^\\S]+",
      "b": ""
    }
  },
  "province": {
    "expr": "content=\"province=([^;]+)",
    "g": 1
  },
  "city": {
    "expr": "content=\"[^;]+;city=([^;]+)",
    "g": 1
  },
  "zone": {
    "expr": "区域</div>([\\s\\S]*?)<span",
    "g": 1,
    "replace": {
      "a": "<div[^>]+>|<a[^>]+>|</a>|&nbsp;|\\r|\\n|\\t",
      "b": ""
    }
  }
}