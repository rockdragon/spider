{
  "title": {
    "expr": "<h1[^>]*>([^<]+)",
    "g": 1
  },
  "publishDate": {
    "expr": "<li[^c]+class=\"time\">\\s+((\\d{4}-\\d{2}-\\d{2})|<script>([^<]+))",
    "g": 1
  },
  "overview": {
    "expr": "<div class=\"su_tit\">概况</div>\\s+<div class=\"su_con\">([^<]+)",
    "g": 1,
    "replace": {
      "a": "[^\\S]+",
      "b": " "
    }
  },
  "province": {
    "expr": "content=\"province=([^;]+)",
    "g": 1
  },
  "city": {
    "expr": "content=\"[^;]+;city=([^;]+)",
    "g": 1
  },
  "zone": {
    "expr": "区域</div>([\\s\\S]*?)<span",
    "g": 1,
    "replace": {
      "a": "<[^>]+>|</[^>]+>|&nbsp;|\\r|\\n|\\t|\\s",
      "b": ""
    }
  },
  "address": {
    "expr": "地址</div>\\s+<div[^>]+>([\\s\\S]*?)<[^>]+>",
    "g": 1,
    "replace": {
      "a": "[^\\S]+",
      "b": ""
    }
  },
  "furniture": {
    "expr": "配置</div>([\\s\\S]*?)</div>",
    "g": 1,
    "replace": {
      "a": "<[^>]+>|</[^>]+>|[^\\S]+",
      "b": ""
    }
  },
  "contact": {
    "expr": "(</a>.*?|<a[^>]+>\\s+\\S+\\s+</a>\\s+)<em class=\"shenfen\">",
    "g": 1,
    "replace": {
      "a": "<[^>]+>|</[^>]+>|[^\\S]+",
      "b": ""
    }
  },
  "cityid": {
    "expr": "locallist:\\[{dispid:'([^']+)'",
    "g": 1
  },
  "infoid":{
    "expr": "}\\],infoid:'([^']+)'",
    "g": 1
  },
  "phone":{
    "expr": "<span id=\"t_phone\"[^>]+>([\\s\\S]*?)</span>",
    "g": 1,
    "replace": {
      "a": "[^\\S]+",
      "b": " "
    }
  }
}