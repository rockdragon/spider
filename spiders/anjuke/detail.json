{
  "title": {
    "expr": "<h3 class=\"fl\">([^<]+)",
    "g": 1
  },
  "publishDate": {
    "expr": "发布时间：([^<]+)",
    "g": 1,
    "replace": [
      {
        "a": "年|月",
        "b": "-"
      },
      {
        "a": "日",
        "b": ""
      }
    ]
  },
  "longitude": {
    "expr": "from=prop_view_popup#l1=[^&]+&l2=([^&]+)",
    "g": 1
  },
  "latitude": {
    "expr": "from=prop_view_popup#l1=([^&]+)&l2=[^&]+",
    "g": 1
  },
  "city": {
    "expr": "<span class=\"city\">([^<]+)",
    "g": 1
  },
  "zone": {
    "expr": "所在版块</dt>\\s+<dd>\\s+([\\s\\S]*?)\\s+</dd>",
    "g": 1,
    "replace": [
      {
        "a": "<[^>]+>|</[^>]+>",
        "b": ""
      },
      {
        "a": "[^\\S]+",
        "b": " - "
      }
    ]
  },
  "address": {
    "expr": [
      {
        "expr": "所在版块</dt>\\s+<dd>\\s+([\\s\\S]*?)\\s+</dd>",
        "g": 1
      },
      {
        "expr": "小区名</dt>\\s+<dd>\\s+<a[^>]+>([^<]+)</a>",
        "g": 1
      }
    ],
    "sep": " - ",
    "replace": [
      {
        "a": "<[^>]+>|</[^>]+>",
        "b": ""
      },
      {
        "a": "\\n\\s+",
        "b": " - "
      }
    ]
  }
}